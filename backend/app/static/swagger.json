{
  "openapi": "3.0.0",
  "info": {
    "title": "Location Extractor API",
    "version": "1.0.0",
    "description": "API for extracting location information from URLs"
  },
  "paths": {
    "/api/extract": {
      "post": {
        "summary": "Extract locations from provided URL",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "url": {
                    "type": "string",
                    "format": "uri",
                    "description": "URL to extract locations from"
                  }
                },
                "required": ["url"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful extraction",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "extracted_locations": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
